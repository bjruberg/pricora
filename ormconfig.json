{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "api/src/entity/**/*.ts"
   ],
   "migrations": [
      "api/src/migration/**/*.ts"
   ],
   "subscribers": [
      "api/src/subscriber/**/*.ts"
   ],
   "migrationsTableName": "migrations"
}