{
  "name": "pricora",
  "version": "0.0.1",
  "description": "Simple installable node.js based web application for handling attendance lists",
  "main": "index.ts",
  "scripts": {
    "build": "concurrently \"npm:build:client\" \"npm:build:server\"",
    "build:server": "NODE_ENV=production tsc --build server/tsconfig.json",
    "build:client": "NODE_ENV=production rollup -c",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "concurrently \"npm:dev:rollup\" \"npm:dev:types\" \"npm:dev:server\"",
    "dev:client": "concurrently \"npm:dev:rollup\" \"npm:dev:types\"",
    "dev:rollup": "NODE_ENV=development rollup -c --watch",
    "dev:redis": "redis-server redis.conf",
    "dev:server": "NODE_ENV=\"development\" ts-node-dev --script-mode --transpile-only --respawn server/src/app.ts",
    "dev:types": "graphql-codegen generate --watch",
    "postinstall": "node-config-ts",
    "start": "NODE_ENV=production node server/build/app.js",
    "test-e2e": "NODE_ENV=\"test\" rollup -c && concurrently \"npm:test:server\" \"npm:cypress:run\"",
    "test:server": "ts-node-dev --script-mode --transpile-only server/src/app.ts",
    "test": "jest"
  },
